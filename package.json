{
  "name": "poliwag",
  "version": "1.0.0",
  "description": "poliwag",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development lsc server.ls --nodejs --harmoy",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.prod.ls",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack --progress --colors --config webpack.config.dev.ls",
    "test": "mocha --compilers js:babel-core/register",
    "travis": "cross-env NODE_ENV=production webpack --config webpack.config.prod.ls",
    "lint": "eslint ./src/",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npm run prettierJS && npm run prettierJSX",
    "prettierJS": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "prettierJSX": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.jsx\""
  },
  "author": "Panepo <panepo@github.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Panepo/Poliwag.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "d3": "^3.5.17",
    "mathjs": "^3.10.1",
    "rd3": "^0.7.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^3.1.0",
    "json-loader": "^0.5.4",
    "livescript": "^1.5.0",
    "livescript-loader": "^0.1.6",
    "mocha": "^5.0.5",
    "node-sass": "^4.3.0",
    "prettier": "1.11.1",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^4.1.1",
    "should": "^13.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  }
}
